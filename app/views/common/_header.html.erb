<header>
  <div class="navbar">
    <div class="navbar-inner">
      <div class="container">
        <%= link_to t('brand'), root_path, :class => 'brand' %>
        <ul class="nav">
          <li><%= link_to "Services", services_path %></li>
          <li><%= link_to "Products", products_path %></li>
          <li><%= link_to "Meet Us", meetus_path %></li>
          <li><%= link_to "Location", location_path %></li>
          <li><%= link_to "Schedule", schedule_path %></li>
        </ul>
        <ul class="nav pull-right">
          <% if signed_in? %>
            <li><%= link_to "Sign Out", signout_path, :method => :delete %></li>              
          <% else %>
            <li><%= link_to "Sign Up", signup_path %>
            <li><%= link_to "Sign In", signin_path %></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</header>